    "@types/object-assign": "^4.0.30",
    "@types/proxyquire": "^1.3.28",
    "@types/tempy": "^0.1.0",
    "@types/uuid": "^3.4.4",
    "eslint": "^4.19.1",
    "jasmine": "^3.1.0",
    "proxyquire": "^2.0.1",
    "tempy": "^0.2.1",
    "typescript": "^3.1.0-dev.20180901"
  },
  "dependencies": {
    "file-type": "^7.7.1",
    "fs-extra": "^6.0.1",
